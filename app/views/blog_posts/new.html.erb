<h1>New Blog Post</h1>

<%= form_with(model: @blog_post, local: true) do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>

  <%= form.label :content %>
  <%= form.text_area :content %>

  <%= form.submit "Create Blog Post" %>
<% end %>

<%= link_to "Back to All Blog Posts", blog_posts_path %>
