<h1> New Post <h1>

<%= form_tag('/userblog/create', :method => "post") do %>
 
  Title: <%= text_field_tag(:title)%>
  Content: <%= text_area_tag(:content)%>
 
  
  <%= hidden_field_tag(:likes, 0)%>
  <%= hidden_field_tag(:dislikes, 0)%>
  <%= hidden_field_tag(:user_id, @user.id) %>

  <%= submit_tag("create new post")%>

<%end %>